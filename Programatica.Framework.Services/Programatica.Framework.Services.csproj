    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>Dotnetcore development framework accelarator</Description>
    <Copyright>Copyright Â©  Programatica Software 2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/ruialexrib/Programatica.Framework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ruialexrib/Programatica.Framework</RepositoryUrl>
    <Version>1.0.3</Version>
    <Authors>ruialexrib</Authors>
    <Company>Programatica Software</Company>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Programatica.Framework.Core" Version="1.0.2" />
    <PackageReference Include="Programatica.Framework.Data" Version="1.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System">
